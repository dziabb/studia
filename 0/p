FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    airinlet
    {
        type            fixedValue;
        value           uniform 0;
    }

    airoutlet
    {
        type            zeroGradient;

    }

    airsymmetry
    {
        type            symmetry;
    }

    airsymmetryside
    {
        type            symmetry;
    }

    default_wall
    {
        type            zeroGradient;
    }

    finwall
    {
        type            zeroGradient;
    }

    pipewall
    {
        type            zeroGradient;
    }
}

